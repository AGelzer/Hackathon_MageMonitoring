<?xml version="1.0" encoding="UTF-8"?>
<config>
    <modules>
        <Hackathon_Magemagemonitoring>
            <version>1.0.0</version>
        </Hackathon_Magemagemonitoring>
    </modules>

    <default>
    </default>

    <global>
        <blocks>
            <magemonitoring>
                <class>Hackathon_MageMonitoring_Block</class>
            </magemonitoring>
        </blocks>

        <models>
            <magemonitoring>
                <class>Hackathon_MageMonitoring_Model</class>
                <resourceModel>cachereset_mysql4</resourceModel>
            </magemonitoring>
            <magemonitoring_mysql4>
                <class>Hackathon_MageMonitoring_Model_Mysql4</class>
                <entities>
                </entities>
            </magemonitoring_mysql4>
            <!-- Fake model rewrite -->
            <catalog>
                <rewrite>
                    <category>Hackathon_MageMonitoring_Model_Category</category>
                </rewrite>
            </catalog>
            <!-- / Fake model rewrite -->
        </models>

        <helpers>
            <magemonitoring>
                <class>Hackathon_MageMonitoring_Helper</class>
            </magemonitoring>
        </helpers>
    </global>

    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <magemonitoring before="Mage_Adminhtml">Hackathon_MageMonitoring_Adminhtml</magemonitoring>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>

    <adminhtml>
        <layout>
            <updates>
                <magemonitoring>
                    <file>monitoring.xml</file>
                </magemonitoring>
            </updates>
        </layout>
    </adminhtml>
</config>